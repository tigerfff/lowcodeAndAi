{
  "name": "hui",
  "version": "2.43.2",
  "files": [
    "lib",
    "src",
    "packages",
    "types"
  ],
  "main": "lib/hui.common.js",
  "unpkg": "lib/hui.umd.min.js",
  "style": "lib/hui.css",
  "author": "xiangxiao3",
  "license": "MIT",
  "bugs": {
    "url": "http://iris.hikvision.com.cn/hui-vue/hui-vue/issues"
  },
  "repository": "http://iris.hikvision.com.cn/hui-vue/hui-vue.git",
  "homePage": "http://hui.dev.hikhub.net/",
  "keywords": [
    "components",
    "hui",
    "common"
  ],
  "cover": "http://hui.dev.hikhub.net/favicon.ico",
  "description": "A hik UI design language and Vue 2 components implementation",
  "changelog": "http://iris.hikvision.com.cn/hui-vue/hui-vue/blob/master/CHANGELOG.md",
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "yarn lint && vuepress build docs",
    "build:all": "yarn test && yarn build && yarn lib:all",
    "lint": "vue-cli-service lint src packages && yarn lint:style",
    "lint:style": "stylelint packages/**/*.scss --fix",
    "cz:commit": "git-cz",
    "lib:all": "yarn lib:clean && yarn lib:i18n && yarn lib:common && yarn lib:umd && yarn lib:esm-browser",
    "lib:clean": "rimraf lib",
    "lib:common": "cross-env LIB_TYPE=common vue-cli-service build --no-clean --target lib --formats commonjs --name hui src/index.js",
    "lib:esm-browser": "cross-env NODE_ENV=esm-browser rollup --config ./rollup.config.js",
    "lib:i18n": "cross-env NODE_ENV=i18n babel src/locale --out-dir lib/locale",
    "lib:umd": "cross-env LIB_TYPE=umd vue-cli-service build --no-clean --target lib --formats umd-min --name hui src/index.js",
    "release": "standard-version",
    "cz:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "build:icon-data": "node docs/.vuepress/public/build/build-icon-data.js && node docs/.vuepress/public/build/build-svg-icon-data.js",
    "test:style": "sass packages/theme:packages/theme/release && rimraf packages/theme/release",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@soda/get-current-script": "^1.0.1",
    "async-validator": "1.6.9",
    "core-js": "^3.6.5",
    "deepmerge": "^1.5.2",
    "global": "^4.4.0",
    "resize-observer-polyfill": "^1.5.0",
    "throttle-debounce": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.18.10",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@hui/eslint-config-vue": "1.0.6",
    "@hui/stylelint-scss": "1.0.2",
    "@hui/svg-icon": "^1.0.3",
    "@rollup/plugin-babel": "^5.0.0",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/test-utils": "^1.0.3",
    "@vuepress/plugin-back-to-top": "^1.0.1",
    "axios": "^0.18.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "commitizen": "^4.1.2",
    "compass-sass-mixins": "^0.12.7",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^5.2.0",
    "css-prefers-color-scheme": "^4.0.0",
    "entities": "1.1.1",
    "eslint": "6.8.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-loader": "3.0.4",
    "git-cz": "^4.7.3",
    "highlightjs": "^9.12.0",
    "husky": "^4.2.5",
    "jest": "^24.8.0",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^8.1.3",
    "live-server": "^1.2.1",
    "mo-js": "^0.288.2",
    "moment": "^2.24.0",
    "normalize-wheel": "^1.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.16.4",
    "raw-loader": "^0.5.1",
    "rollup": "^1.16.7",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-vue": "^5.0.1",
    "sass": "^1.33.0",
    "sass-loader": "^10.0.0",
    "sass-migrator": "^1.4.0",
    "standard-version": "^8.0.0",
    "stylelint-webpack-plugin": "1.2.3",
    "typescript": "3.5.2",
    "unplugin-vue-jsx": "^0.1.1",
    "vue": "~2.6.14",
    "vue-server-renderer": "~2.6.14",
    "vue-cli-plugin-changelog": "^1.1.9",
    "vue-cli-plugin-lint-staged": "^0.1.1",
    "vue-jest": "^4.0.0-beta.2",
    "vue-template-compiler": "~2.6.14",
    "vuepress": "^1.0.0",
    "vuepress-plugin-lego-analytics": "^0.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "defaults"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-merge": "yarn",
      "pre-commit": "lint-staged"
    }
  },
  "typings": "types/index.d.ts"
}
