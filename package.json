{
  "name": "ai-code",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint src --ext .vue,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{vue,js,jsx,css,json}\"",
    "prepare": "husky",
    "validate:components": "node scripts/validate-components.js",
    "validate:templates": "node scripts/validate-templates.js",
    "validate:all": "npm run validate:components && npm run validate:templates"
  },
  "lint-staged": {
    "*.{vue,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vue/compiler-sfc": "^3.5.22",
    "element-plus": "^2.11.5",
    "handlebars": "^4.7.8",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.22",
    "vue-template-compiler": "^2.7.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^2.1.9",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.16",
    "vite": "^6.0.11",
    "vitest": "^2.1.9"
  }
}
